<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsi.ant.sample.backgroundscan"
    android:versionCode="010100"
    android:versionName="01.01.00" >
    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:allowBackup="true"
         >
        
        <activity
            android:name="com.dsi.ant.sample.backgroundscan.ChannelList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service android:name="com.dsi.ant.sample.backgroundscan.ChannelService"></service>
    </application>
</manifest>